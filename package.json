{
  "name": "kb-components",
  "version": "1.5.7",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:lib:copy": "copyfiles README.md LICENSE  ./projects/kibibit/kb-components/",
    "build:package-copy": "node ./copy-attributes.js",
    "build:lib": "ng build @kibibit/kb-components",
    "pack": "cd dist/kibibit/kb-components && npm pack",
    "build:rename": "renamer --find /kibibit-kb-components-.*\\.tgz/ --replace kibibit-kb-components.tgz ./dist/kibibit/kb-components/*",
    "package": "npm run build:lib && npm run build:lib:copy && npm run build:package-copy && npm run pack && npm run build:rename",
    "pub": "npm run package && npm publish ./dist/kibibit/kb-components/kibibit-kb-components.tgz",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate",
    "docs": "ng build kb-component-docs --configuration=production --aot"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Kibibit/kb-components"
  },
  "homepage": "http://kibibit.io/kb-components/",
  "private": false,
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/cdk": "~7.3.2",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/material": "^7.3.2",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "chance": "^1.0.18",
    "core-js": "^2.5.4",
    "dexie": "^2.0.4",
    "hammerjs": "^2.0.8",
    "highlight.js": "^9.14.2",
    "moment": "^2.24.0",
    "ngx-highlightjs": "^3.0.3",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.12.0",
    "@angular-devkit/build-ng-packagr": "~0.12.0",
    "@angular/cli": "~7.2.3",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@types/chance": "^1.0.1",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^8.9.5",
    "all-contributors-cli": "^6.1.1",
    "codelyzer": "~4.5.0",
    "copyfiles": "^2.1.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^4.2.0",
    "protractor": "~5.4.0",
    "renamer": "^1.1.1",
    "replace": "^1.0.1",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.34.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2",
    "write-json-file": "^3.1.0"
  }
}
